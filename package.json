{
  "name": "@decentralized-geo/astral-sdk",
  "version": "0.1.0",
  "description": "Astral's Location Proof Protocol SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./offchain": {
      "types": "./dist/offchain/index.d.ts",
      "import": "./dist/offchain/index.mjs",
      "require": "./dist/offchain/index.js"
    },
    "./onchain": {
      "types": "./dist/onchain/index.d.ts",
      "import": "./dist/onchain/index.mjs",
      "require": "./dist/onchain/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "release": "changeset publish",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "commit-wip": "git commit --no-verify -m \"WIP: Work in progress commit\""
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "location",
    "proof",
    "attestation",
    "eas"
  ],
  "author": "Astral Protocol",
  "license": "Apache-2.0",
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^2.7.0",
    "@turf/boolean-valid": "7.2.0",
    "@turf/meta": "7.2.0",
    "@types/geojson": "7946.0.16",
    "ethers": "^6.13.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.4",
    "@types/jest": "29.5.14",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv": "16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "11.0.2",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "license-checker": "25.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "4.19.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
